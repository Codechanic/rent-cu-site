<!-- BEGIN PAGE CONTAINER-->

    <!-- BEGIN PAGE HEADER-->
    <div class="row">
        <div class="col-md-12">     
            <!-- BEGIN PAGE TITLE & BREADCRUMB-->
           <h3 class="page-title">
                {{ 'admin.subscriber.module' | trans }} <small> {{ 'admin.subscriber.description' | trans }}</small>
            </h3>
           <ul class="page-breadcrumb breadcrumb">
                <li>
                    <i class="icon-home"></i>
                   <a href="{{ path('admin_index') }}">Dashboard</a>
                    <i class="icon-angle-right"></i>
                </li>
                
                <li class="active">{{ 'admin.subscriber.module' | trans }}</li>
            </ul>
            <!-- END PAGE TITLE & BREADCRUMB-->
        </div>
    </div>
    <!-- END PAGE HEADER-->
    <div class="row">
        <div class="col-md-12 admin-entity-viewer" id="admin-subscriber-viewer">
           {% include 'MainBundle:Subscriber:list.html.twig' %}
        </div>
    </div>

    <script type="text/javascript">

    $('a[action=list]').on('click', function(e) {
        var url=$(this).attr('url');
        var parent=$(this).attr('data-parent_name');
        e.preventDefault();
        var pageContent = $('.admin-entity-viewer');
        App.blockUI(pageContent, false);
        $('.admin-entity-viewer').load(url, function() {
        $('.inbox-nav > li.active').removeClass('active');
        $('.inbox-nav > li.' + parent).addClass('active');
        App.unblockUI(pageContent);
        });
         });
    </script>
    
 
